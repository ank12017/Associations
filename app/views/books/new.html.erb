<h1>new book</h1>
<%= form_for @book , :html => {:multipart => true} do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :author_id %><br>
    <%= form.text_field :author_id %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
<%= link_to 'Back', books_path %>